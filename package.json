{
  "private": true,
  "version": "1.0.0",
  "name": "nate.challenge.ios",
  "scripts": {
    "dev": "concurrently \"yarn dev:graph\" \"yarn dev:rest\"",
    "dev:graph": "ts-node-dev --tree-kill --no-notify --respawn --transpile-only -r tsconfig-paths/register api/graph.ts",
    "dev:rest": "ts-node-dev -r tsconfig-paths/register api/rest.ts",
    "build": "yarn -s clean && yarn -s generate && tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "generate": "yarn -s generate:prisma && yarn -s generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NODE_ENV=development ts-node --transpile-only api/schema/schema",
    "reset": "ts-node -r tsconfig-paths/register api/seed.ts",
    "start": "concurrently \"yarn start:graph\" \"yarn start:rest\"",
    "start:graph": "node .nexus/build/api",
    "start:rest": "ts-node -r tsconfig-paths/register api/rest.ts",
    "lint": "eslint '*/**/*.{js,ts}'",
    "format": "yarn lint --quiet --fix"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@prisma/client": "2.23.0",
    "apollo-server": "2.24.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "0.35.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/node": "15.6.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "concurrently": "6.2.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.0",
    "prisma": "2.23.0",
    "ts-node": "10.0.0",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  }
}
